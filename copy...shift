//Shift
#include<PIC18F4550.h>
void main()
{
    int arr1[]={5 , 12 , 7 , 9};
    int arr2[]={15 , 22 ,27 , 38};
    int temp;
    for(int i = 0 ; i < 4 ; i++)
    {
        temp = arr1[i];
        arr1[i] = arr2[i];
        arr2[i] = temp;
    }
}

//Copy
#include<PIC18F4550.h>
void main()
{
    int arr1[]={5 , 12 , 7 , 9};
    int arr2[]={0 , 0 , 0 , 0};
    for(int i = 0 ; i < 4 ; i++)
    {
        arr2[i] = arr1[i];
    }
}
